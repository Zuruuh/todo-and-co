{
    "name": "saro0h/to-do-list",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.2.5",
        "symfony/symfony": "5.3.*",
        "doctrine/orm": "^2.10",
        "doctrine/doctrine-bundle": "^2.4",
        "symfony/swiftmailer-bundle": "^3.5",
        "symfony/monolog-bundle": "^3.7",
        "symfony/polyfill-apcu": "^1.23",
        "sensio/framework-extra-bundle": "^6.2.1",
        "incenteev/composer-parameter-handler": "^2.1",
        "symfony/flex": "^1.17",
        "symfony/twig-bundle": "5.3.*",
        "symfony/security-bundle": "5.3.*"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^3.0",
        "phpunit/phpunit": "^9.5"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.3.*"
        }
    }
}
